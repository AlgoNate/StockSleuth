{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["timestamp", "stocks"],
    "properties": {
      "timestamp": {
        "type": "string",
        "format": "date-time"
      },
      "stocks": {
        "type": "array",
        "items": {
          "type": "object",
          "required": ["symbol", "name", "price", "percent_change", "history"],
          "properties": {
            "symbol": { "type": "string" },
            "name": { "type": "string" },
            "price": { "type": "number" },
            "percent_change": { "type": "number" },
            "history": {
              "type": "array",
              "items": { "type": "number" }
            }
          }
        }
      }
    }
  }
}
